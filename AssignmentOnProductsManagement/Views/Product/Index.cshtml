@model List<AssignmentOnProductsManagement.Models.Products>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<a href="Product/Create" >Create</a>
<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Product Name</th>
            <th>Product Price</th>
            <th>Stock</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Pid</td>
                <td>@item.Pname</td>
                <td>@item.Price</td>
                <td>@item.Stock</td>
            </tr>
}
    </table>


</div>
